{
  "name": "@paras205/react-ts-starter",
  "bin": "./bin/cli.js",
  "version": "0.0.14",
  "type": "commonjs",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/paras205/boilerplate-react.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky install",
    "prebuild": "yarn format && yarn lint",
    "commit": "git-cz",
    "postinstall": "npx husky install",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file --release-count 0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@ckeditor/ckeditor5-build-classic": "^39.0.2",
    "@ckeditor/ckeditor5-react": "^6.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-table": "^8.9.7",
    "@types/draft-js": "^0.11.12",
    "@types/js-cookie": "^3.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-scroll": "^1.8.7",
    "@types/react-table": "^7.7.15",
    "axios": "^1.4.0",
    "draft-js": "^0.11.7",
    "esbuild-plugins-node-modules-polyfill": "^1.6.0",
    "framer-motion": "^10.16.2",
    "html-react-parser": "^4.2.2",
    "i": "^0.3.7",
    "i18next": "^21.9.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.34.2",
    "react-hot-toast": "^2.3.0",
    "react-i18next": "^11.18.5",
    "react-icons": "^4.10.1",
    "react-moment": "^1.1.3",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.15.0",
    "react-scroll": "^1.8.9",
    "react-select": "^5.7.4",
    "react-table": "^7.8.0",
    "recharts": "^2.8.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "styled-components": "^6.0.7",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": ">=5.31.0",
    "@typescript-eslint/parser": ">=5.31.0",
    "@vitejs/plugin-react": "^2.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": ">=8.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-typescript": "^0.53.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": ">=4.4",
    "vite": "^3.0.7",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-merge": "if [ $HUSKY_GIT_PARAMS = 'develop' ]; then yarn; fi"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
